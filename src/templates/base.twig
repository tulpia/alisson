<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="{{ fn("bloginfo", "charset") }}">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="profile" href="https://gmpg.org/xfn/11">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    
    {{ function('wp_head') }}
</head>
<body class="">
    <header class="wrapper">
        {% block header %}
            {% include "partial/header.twig" %}
        {% endblock %}
    </header>

    <main class="wrapper" data-scroll-container>
        {% block content %}
            content goes here
        {% endblock %}

        <footer data-scroll-section class="wrapper-small">
            {% block footer %}
                {% include "partial/footer.twig" %}
            {% endblock %}
        </footer>
    </main>

    {{ function('wp_footer') }}
</body>
</html>